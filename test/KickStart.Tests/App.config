<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.diagnostics>
    <trace autoflush="true">
      <listeners>
        <add name="traceFile"
             type="System.Diagnostics.TextWriterTraceListener"
             initializeData="d:\trace.log" />
      </listeners>
    </trace>
        
    <switches>
      <add name="KickStart" value="Verbose" />
    </switches>

  </system.diagnostics>
</configuration>